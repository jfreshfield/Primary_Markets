{
  "name": "<%= name %>",
  "version": "1.1.0",
  "description": "Marketplace Frontend",
  "sideEffects": false,
  "config": {
    "build_dir": "marketplace_builder/assets"
  },
  "scripts": {
    "postinstall": "npm run build:dev",
    "start": "npm run build:dev -- -w",
    "sync": "marketplace-kit sync -e",
    "clean": "echo 'Clearing build directory' & rm -rf $npm_package_config_build_dir/*",
    "copy-static": "cp -a src/static/. $npm_package_config_build_dir/",
    "build:dev": "webpack",
    "prebuild:production": "npm run clean; npm run copy-static",
    "build:production": "NODE_ENV=production webpack -p",
    "deploy": "marketplace-kit deploy -e $npm_package_config_target",
    "deploy:staging": "npm run build:production && npm run deploy --<%= name %>:target=staging",
    "deploy:production": "npm run build:production && npm run deploy --<%= name %>:target=production"
  },
  "browserslist": ["last 2 versions", "iOS > 7", "Safari > 6"],
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "autoprefixer": "^8.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "glob-all": "^3.1.0",
    "node-sass": "4.8.3",
    "postcss": "^6.0.21",
    "postcss-calc": "^6.0.1",
    "postcss-csso": "^3.0.0",
    "postcss-devtools": "^1.1.1",
    "postcss-fixes": "^2.0.1",
    "postcss-focus": "^3.0.0",
    "postcss-loader": "^2.1.3",
    "postcss-pseudoelements": "^5.0.0",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-require-from": "^1.2.3"
  },
  "dependencies": {
    "rails-ujs": "^5.1.6",
    "vanilla-lazyload": "^10.4.2"
  }
}
